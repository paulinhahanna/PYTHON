#outra forma de INTERPOLAR

nome = "geisa"
salario = 4500.99

print(nome,"ganha um salario de R$", salario)
print (f'{nome} ganha um salario de R$ {salario}')

#forma FORMAT de imprimir
# s - string
# d e i -int
# f - float
# x e X - hexadecimais
print ('o salario de %s Ã© R$ %.2f' % (nome, salario))